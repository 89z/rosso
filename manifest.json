   "content_scripts": [
      {
         "js": ["youtube.js"],
         "matches": ["<all_urls>"]
      }
   ],
   "manifest_version": 2,
   "name": "Rosso",
   "permissions": ["https://www.youtube.com/"],
   "version": "1.0.0"
}
