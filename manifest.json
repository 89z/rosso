{
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "rosso.png"
   },
   "browser_specific_settings": {
      "gecko": {"id": "rosso@89z"}
   },
   "content_scripts": [
      {
         "js": [
            "delay.js",
            "youtube.js",
            "soundcloud.js",
            "backblaze.js"
         ],
         "matches": ["<all_urls>"]
      }
   ],
   "manifest_version": 2,
   "name": "Rosso",
   "permissions": [
      "https://api-v2.soundcloud.com/",
      "https://www.youtube.com/"
   ],
   "version": "1.2.3"
}
